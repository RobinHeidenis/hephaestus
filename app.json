{
  "expo": {
    "name": "Hephaestus",
    "slug": "hephaestus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hephaestus",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#25232A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robinheidenis.hephaestus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#25232A"
      },
      "package": "com.robinheidenis.hephaestus"
    },
    "androidNavigationBar": {
      "backgroundColor": "#2C2831"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-share-intent"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1fddc2ad-8050-4ce2-9f20-8a6073ade08c",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "ShareExtension",
                  "bundleIdentifier": "undefined.share-extension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.undefined"
                    ]
                  }
                }
              ]
            }
          }
        }
      }
    },
    "owner": "robinheidenis"
  }
}
